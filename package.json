{
  "name": "weapp-finder",
  "version": "0.0.2",
  "description": "管理微信小程序项目",
  "repository": {
    "type": "git",
    "url": "git@github.com:RobGu/vscode-weapp-finder.git"
  },
  "license": "MIT",
  "main": "./out/extension.js",
  "scripts": {
    "compile": "babel -s -d out src",
    "lint": "eslint src --fix",
    "package": "vsce package --yarn",
    "vscode:prepublish": "npm run compile",
    "watch": "babel -w -s -d out src"
  },
  "dependencies": {
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/register": "7.4.4",
    "@types/node": "*",
    "@types/vscode": "1.34.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.13.0",
    "husky": "2.3.0",
    "lint-staged": "8.1.7",
    "prettier": "1.17.1"
  },
  "engines": {
    "vscode": "^1.32.0"
  },
  "activationEvents": [
    "onView:h5Explorer",
    "onView:weappExplorer",
    "onLanguage:json",
    "onLanguage:jsonc",
    "onCommand:sample.reveal"
  ],
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "h5Explorer.refreshFile",
        "icon": {
          "dark": "resources/dark/refresh.svg",
          "light": "resources/light/refresh.svg"
        },
        "title": "Refresh"
      },
      {
        "command": "h5Explorer.openFile",
        "title": "Open File"
      },
      {
        "command": "weappExplorer.reveal",
        "title": "Test View: Reveal"
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "command": "h5Explorer.refreshFile",
          "group": "inline",
          "when": "view == h5Explorer && viewItem == file"
        }
      ]
    },
    "views": {
      "weapp-finder": [
        {
          "name": "H5 v2",
          "id": "h5Explorer"
        },
        {
          "name": "微信小程序",
          "id": "weappExplorer"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "icon": "media/dep.svg",
          "id": "weapp-finder",
          "title": "小程序管理"
        }
      ]
    }
  },
  "displayName": "小程序工程管理",
  "publisher": "ggbb"
}
